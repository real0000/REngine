<root isCompute="false" isIndexed="true">
  <ParamList>
    <ConstBuffer name="g_Camera" preDef="CameraBuffer" reserved="true"/>
    <UavBuffer name="m_SkinTransition" preDef="SkinMatrix" write="false"/>
    <UavBuffer name="InstanceInfo" write="false">
      <!--customize batch draw info here, m_World, m_Local, m_VtxFlag, m_SkinMatBase are required-->
      <Param name="m_World" desc="" type="float4x4"/>
      <Param name="m_VtxFlag" desc="" type="int1"/>
      <Param name="m_SkinMatBase" desc="" type="int1"/>
    </UavBuffer>
    <Srv2D name="m_DiffTex" desc="" reserved="false"/>
    <Srv2D name="m_NormalTex" desc="" reserved="false"/>
  </ParamList>
  <Shaders>
    <Model_5_1>
      <Vertex file="VertexTransfer.vertex">
        g_Camera, m_SkinTransition, InstanceInfo
      </Vertex>
      <!--Geometry file="">
      </Geometry>
      <TessCtrl file="">  
      </TessCtrl>
      <TessEval file="">
      </TessEval>
      <Compute>
      </Compute>-->
      <Fragment file="TextureOnly.pixel">
        m_DiffTex, m_NormalTex
      </Fragment>
    </Model_5_1>
  </Shaders>
  <ShaderSetting sampleMask="ffffffff" topology="triangle_list" sampleCount="1" sampleQuality="0" useIndirectDraw="true">
    <BlendState/>
    <RasterizerState filled="true" cull="none" ccFront="false" depthBias="0" slopeDepthBias="0.0" depthBiasClamp="0.0" useDepthClip="true" useMultiSample="false" lineaa="false" forceSampleCount="0" conservative="false"/>
    <DepthStencil>
      <Depth enable="true" write="true" func="less"/>
      <Stencil enable="false" readMask="ff" writeMask="ff">
        <Front func="always" depthFailOP="keep" passOP="keep" failOP="keep"/>
        <Back func="always" depthFailOP="keep" passOP="keep" failOP="keep"/>
      </Stencil>
    </DepthStencil>
    <RenderTarget depthStencil="d24_unorm_s8_uint">
      <rgba8_snorm/>
      <rgba8_uint/>
      <rgba8_unorm/>
      <rgba8_unorm/>
      <rgba8_unorm/>
      <rg16_float/>
    </RenderTarget>
  </ShaderSetting>
</root>