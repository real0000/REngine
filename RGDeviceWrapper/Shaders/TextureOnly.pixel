#include <StdInclude.h>
#include "autoBinder.h"

PS_CommonOutput main(float2 a_UV : TEXCOORD0) : SV_TARGET0
{
	PS_CommonOutput l_Output = (PS_CommonOutput)0;

	l_Output.m_Target0.xyz = encodeNormal(m_NormalTex.Sample(s_LinearSampler, a_UV));
	l_Output.m_Target2.xyz = m_DiffTex.Sample(s_LinearSampler, a_UV).xyz;

	return l_Output;
}